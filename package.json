{
  "name": "imgage-gallery",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://josephchinedu.github.io/image-gallery/",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "gh-pages": "^3.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.3"
  },
  "scripts": {
    "start": "npm run watch:css && react-scripts start",
    "build": "npm run build:css && react-scripts gh-pages -d build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm build",
    "deploy": "gh-pages -d build",
    "build:css": "postcss src/assests/tailwind.css -o src/assests/main.css",
    "watch:css": "postcss src/assests/tailwind.css -o src/assests/main.css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "postcss-cli": "^7.1.2",
    "tailwindcss": "^1.8.8"
  }
}
